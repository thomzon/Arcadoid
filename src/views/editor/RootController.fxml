<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.editor.RootController">
   <top>
      <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <items>
          <Button mnemonicParsing="false" onAction="#saveToFileAction" text="%action.saveToFile" />
            <Button fx:id="sendToRepositoryButton" mnemonicParsing="false" onAction="#sendToRepositoryAction" text="%action.sendToRepo" />
            <Button fx:id="getFromRepositoryButton" mnemonicParsing="false" onAction="#getFromRepositoryAction" text="%action.getFromRepo" />
        </items>
      </ToolBar>
   </top>
</BorderPane>
